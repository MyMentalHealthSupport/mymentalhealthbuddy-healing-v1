# MyMentalHealthBuddy™ V10·PERFECTION Environment Configuration
PORT=5000
NODE_ENV=development
CORS_ORIGIN=http://localhost:5173

# Security
JWT_SECRET=your-super-secure-jwt-secret-change-in-production
BCRYPT_ROUNDS=12

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# OpenAI TTS
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_TTS_MODEL=tts-1-hd
OPENAI_TTS_VOICE=nova
OPENAI_TTS_FORMAT=mp3

# AWS Polly TTS (Fallback)
AWS_POLLY_REGION=us-east-1
AWS_POLLY_ACCESS_KEY_ID=your-aws-access-key
AWS_POLLY_SECRET_ACCESS_KEY=your-aws-secret-key

# Stripe Advanced Billing
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_PRICE_ID=price_your-stripe-price-id
STRIPE_SUCCESS_URL=http://localhost:5173/success
STRIPE_CANCEL_URL=http://localhost:5173/cancel
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
STRIPE_CUSTOMER_PORTAL_URL=https://billing.stripe.com/p/login/your-portal

# AWS S3 Storage with Deduplication
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
S3_BUCKET=your-s3-bucket-name
S3_CDN_URL=https://your-cloudfront-domain.cloudfront.net
STORAGE_DEDUPLICATION=true

# Monitoring and Analytics
METRICS_ENABLED=true
HEALTH_CHECK_INTERVAL=30000
LOG_LEVEL=info

